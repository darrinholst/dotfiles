          "bin": "/bin/bash",
          "args": [
            "-c",
            "(pkill -f test-vpn.sh || true) && cd /Users/darrinholst/.vpn && ./test-vpn.sh"
          ],
          "env": {
            "PATH": "/usr/bin:/usr/local/bin:/sbin:/bin"
          }
        }
      },
      {
