Include ~/.orbstack/ssh/config

Host cloud.dev
  HostName 10.2.9.31
  User ubuntu
  IdentityFile ~/.ssh/holst-dev.pem

Host cloud-dev-holst
  HostName 10.2.9.31
  IdentityFile ~/.ssh/holst-dev.pem
  RequestTTY yes
  RemoteCommand tmux -2 -u attach -t 0 || tmux -2 -u new -s 0

Host homeassistant
  HostName homeassistant.local
  User root
